{
  "name": "@dreizehn/firebase-client",
  "version": "1.0.0",
  "private": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "scripts": {
    "build": "pnpm clean && run-p -l build:*",
    "build:cjs": "tsc --project . --module commonjs --outDir ./cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./esm",
    "clean": "rimraf esm cjs",
    "lint": "tsc -p . --noEmit"
  },
  "dependencies": {
    "firebase": "10.7.2"
  }
}
